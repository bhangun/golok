import 'package:flutter/material.dart';
import 'package:go_router/go_router.dart';

import '../../core/modules/menu.dart';
import '../../core/modules/module_model.dart';
import '../../core/routes/routes.dart';
import 'pages/<%= snakeCase %>_list.dart';
import 'pages/<%= snakeCase %>_detail.dart';
import 'pages/<%= snakeCase %>_form.dart';

class <%= titleCase %>Module implements Module {
  @override
  String? name = '<%= titleCase %>';
  static const String <%= camelCase %>List = '/<%= camelCase %>List';
  static const String <%= camelCase %>Detail = '/<%= camelCase %>Detail';
  static const String <%= camelCase %>Form = '/<%= camelCase %>Form';

  @override
  pages(BuildContext context) {
    return [
      const Menu(title: '<%= titleCase %> Detail', path: <%= titleCase %>Module.<%= camelCase %>Detail),
      const Menu(title: '<%= titleCase %> Form', path: <%= titleCase %>Module.<%= camelCase %>Form),
      const Menu(
          title: '<%= titleCase %> List',
          path: <%= titleCase %>Module.<%= camelCase %>List,
          showInDrawer: true,
          showInHome: true)
    ];
  }

  @override
  services() {}

  @override
  goroutes() => [];

  @override
  List<StatefulShellBranch> branches() => [
    Routes.shellBranch(<%= camelCase %>List, <%= camelCase %>List,<%= titleCase %>List()),
    Routes.shellBranch(<%= camelCase %>Detail, <%= camelCase %>Detail,<%= titleCase %>Detail()),
    Routes.shellBranch(<%= camelCase %>Form, <%= camelCase %>Form,<%= titleCase %>Form()),
  ];
}
