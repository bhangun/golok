import 'package:flutter/material.dart';
import 'package:go_router/go_router.dart';

import '../../utils/modules/module_model.dart';
import '../../models/menu.dart';
import '../../utils/routes.dart';

import 'services/<%= snackCase %>_services.dart';

class <%= titleCase %>Module implements Module {
  @override
  String? name = '<%= titleCase %>';
  static const String <%= camelCase %>List = '/<%= camelCase %>List';
  static const String <%= camelCase %>Detail = '/<%= camelCase %>Detail';
  static const String <%= camelCase %>Form = '/<%= camelCase %>Form';

  @override
  pages() {
    return [
      Menu(title: '<%= titleCase %> Detail', path: <%= titleCase %>Routes.<%= camelCase %>Detail),
      Menu(title: '<%= titleCase %> Form', path: <%= titleCase %>Routes.<%= camelCase %>Form),
      Menu(
          title: '<%= titleCase %> List',
          path: <%= titleCase %>Routes.<%= camelCase %>List,
          showInDrawer: true,
          showInHome: true)
    ];
  }

  @override
  services() {}

  @override
  goroutes() => [];

  @override
  List<StatefulShellBranch> branches() => [
    Routes.shellBranch(<%= camelCase %>List, <%= titleCase %>List()),
    Routes.shellBranch(<%= camelCase %>Detail, <%= titleCase %>Detail()),
    Routes.shellBranch(<%= camelCase %>Form, <%= titleCase %>Form()),
  ];
}
