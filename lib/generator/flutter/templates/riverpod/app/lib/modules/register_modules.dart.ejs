import 'package:flutter/material.dart';
import 'package:go_router/go_router.dart';

import '../models/menu.dart';
import 'main/main_module.dart';<% if(entities.length > 1) { for (i in entities) { %>
import 'package:<%= entities[i].model.info.name %>/modules/<%= entities[i].snackCase %>/<%= entities[i].snackCase %>_module.dart'; <% }} %>


List<Menu> registerPages(BuildContext context){
  return [
    ...MainModule().pages(context),<% if(entities.length > 1) { for (i in entities) { %>
    ...<%= entities[i].name %>Module().pages(context),  <% }} %>
  ];
}

List<List<GoRoute>> modulesGoroutes()=>[
  MainModule().goroutes(),<% if(entities.length > 1) { for (i in entities) { %>
  <%= entities[i].name %>Module().goroutes(),  <% }} %>
];

List<List<StatefulShellBranch>> modulesBranches(){
  return [
    MainModule().branches(),<% if(entities.length > 1) { for (i in entities) { %>
  <%= entities[i].name %>Module().branches(),  <% }} %>    
  ];
}