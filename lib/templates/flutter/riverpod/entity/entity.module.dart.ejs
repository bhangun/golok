import 'package:flutter/widgets.dart';

import '../../utils/modules/module_model.dart';
import '../../models/menu.dart';
import '<%= nameFile %>_routes.dart';
import 'services/<%= nameFile %>_services.dart';

class <%= nameClass %>Module implements Module {
  @override
  String? name = '<%= nameClass %>';

  @override
  pages() {
    return [
      Menu(title: '<%= nameClass %> Detail', path: <%= nameClass %>Routes.<%= nameField %>Detail),
      Menu(title: '<%= nameClass %> Form', path: <%= nameClass %>Routes.<%= nameField %>Form),
      Menu(
          title: '<%= nameClass %> List',
          path: <%= nameClass %>Routes.<%= nameField %>List,
          showInDrawer: true,
          showInHome: true)
    ];
  }

  @override
  services() {}

  @override
  goroutes() => <%= nameClass %>Routes.goroutes;

  @override
  String? baseRoute = '';
}
