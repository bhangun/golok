
application {
  config {
    baseName app3
  }
  entities * except A, B, C, D, F
  service * with serviceClass
}
<% for (idx in entities) { %>
/**
 * <%= entities[idx].doc %><% if(entities[idx].author){ %>
 * @author <%= entities[idx].author %><%}%>
 */
entity <%= entities[idx].nameTitleCase %> {<% const props = entities[idx].properties; for (px in props) { %> 
  <%= props[px].name %> <%= props[px].dartType %> <%}%>
}
<%}%>
<% for (idx in enums) { %>
enum <%= enums[idx] %> {
  BELGIUM,
  FRANCE,
  ITALY
}
<%}%>

relationship OneToOne {
  A to B,
  B to C,
  C to D,
  D to A
}

dto A, B with mapstruct

paginate A with infinite-scroll
paginate B with pagination
paginate C with pager  // pager is only available in AngularJS

service A with serviceClass
service C with serviceImpl